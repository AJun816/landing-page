{"ast":null,"code":"import { ref, onMounted, onUnmounted, computed } from 'vue';\n\n// ÈÖçÁΩÆÂèÇÊï∞\n\nexport default {\n  __name: 'CircularCountdown',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const countdownConfig = {\n      label: 'üî• Ê¥ªÂä®ÁÅ´ÁÉ≠ËøõË°å‰∏≠ÔºÅÂÄíËÆ°Êó∂ üî•',\n      totalDuration: 60000,\n      // ÊÄªÊó∂ÈïøÔºåÊØ´Áßí\n      labels: {\n        minute: 'ÂàÜ',\n        second: 'Áßí',\n        millisecond: 'ÊØ´Áßí'\n      }\n    };\n\n    // ÂÄíËÆ°Êó∂Áä∂ÊÄÅ\n    const remainingTime = ref(countdownConfig.totalDuration);\n    const minutes = ref('00');\n    const seconds = ref('00');\n    const milliseconds = ref('000');\n    const progress = ref(100);\n    const intervalId = ref(null);\n\n    // Âä®ÁîªÁä∂ÊÄÅ\n    const minuteChanged = ref(false);\n    const secondChanged = ref(false);\n    const millisecondChanged = ref(false);\n\n    // ËÆ°ÁÆó‰ΩéÊó∂Èó¥Áä∂ÊÄÅ (ÊúÄÂêé20Áßí)\n    const isLowTime = computed(() => {\n      return remainingTime.value <= 20000 && remainingTime.value > 0;\n    });\n\n    // ËÆ°ÁÆóÊûÅ‰ΩéÊó∂Èó¥Áä∂ÊÄÅ (ÊúÄÂêé10Áßí)\n    const isVeryLowTime = computed(() => {\n      return remainingTime.value <= 10000 && remainingTime.value > 0;\n    });\n\n    // Ê†ºÂºèÂåñÊó∂Èó¥\n    const formatTime = () => {\n      const ms = remainingTime.value;\n\n      // ‰øùÂ≠òÊóßÂÄºÁî®‰∫éÊ£ÄÊµãÂèòÂåñ\n      const oldMinutes = minutes.value;\n      const oldSeconds = seconds.value;\n      const oldMilliseconds = milliseconds.value;\n\n      // ËÆ°ÁÆóÂêÑÊó∂Èó¥Âçï‰Ωç\n      const mins = Math.floor(ms / 60000);\n      const secs = Math.floor(ms % 60000 / 1000);\n      const msds = Math.floor(ms % 1000);\n\n      // Ê†ºÂºèÂåñÊòæÁ§∫ÔºàË°•Èõ∂Ôºâ\n      minutes.value = mins.toString().padStart(2, '0');\n      seconds.value = secs.toString().padStart(2, '0');\n      milliseconds.value = msds.toString().padStart(3, '0');\n\n      // Êõ¥Êñ∞ËøõÂ∫¶Êù°\n      progress.value = ms / countdownConfig.totalDuration * 100;\n\n      // Ê£ÄÊµãÂèòÂåñÂπ∂Ëß¶ÂèëÂä®Áîª\n      minuteChanged.value = oldMinutes !== minutes.value;\n      secondChanged.value = oldSeconds !== seconds.value;\n      millisecondChanged.value = oldMilliseconds !== milliseconds.value;\n\n      // ÈáçÁΩÆÂä®ÁîªÁä∂ÊÄÅ\n      if (minuteChanged.value) {\n        setTimeout(() => minuteChanged.value = false, 500);\n      }\n      if (secondChanged.value) {\n        setTimeout(() => secondChanged.value = false, 500);\n      }\n      if (millisecondChanged.value) {\n        setTimeout(() => millisecondChanged.value = false, 500);\n      }\n    };\n\n    // ÂºÄÂßãÂÄíËÆ°Êó∂\n    const startCountdown = () => {\n      // Ê∏ÖÈô§ÂèØËÉΩÂ≠òÂú®ÁöÑÂÆöÊó∂Âô®\n      if (intervalId.value) {\n        clearInterval(intervalId.value);\n      }\n\n      // ËÆæÁΩÆÂÆöÊó∂Âô®ÔºåÊØè10ÊØ´ÁßíÊõ¥Êñ∞‰∏ÄÊ¨°\n      intervalId.value = setInterval(() => {\n        remainingTime.value -= 10;\n        if (remainingTime.value <= 0) {\n          remainingTime.value = 0;\n          clearInterval(intervalId.value);\n        }\n        formatTime();\n      }, 10);\n    };\n\n    // ÁªÑ‰ª∂ÊåÇËΩΩÊó∂ÂàùÂßãÂåñÂπ∂ÂºÄÂßãÂÄíËÆ°Êó∂\n    onMounted(() => {\n      formatTime();\n      startCountdown();\n    });\n\n    // ÁªÑ‰ª∂Âç∏ËΩΩÊó∂Ê∏ÖÈô§ÂÆöÊó∂Âô®\n    onUnmounted(() => {\n      if (intervalId.value) {\n        clearInterval(intervalId.value);\n      }\n    });\n\n    // Êö¥Èú≤ÁªôÊ®°Êùø\n    const countdown = countdownConfig;\n    const __returned__ = {\n      countdownConfig,\n      remainingTime,\n      minutes,\n      seconds,\n      milliseconds,\n      progress,\n      intervalId,\n      minuteChanged,\n      secondChanged,\n      millisecondChanged,\n      isLowTime,\n      isVeryLowTime,\n      formatTime,\n      startCountdown,\n      countdown,\n      ref,\n      onMounted,\n      onUnmounted,\n      computed\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","computed","countdownConfig","label","totalDuration","labels","minute","second","millisecond","remainingTime","minutes","seconds","milliseconds","progress","intervalId","minuteChanged","secondChanged","millisecondChanged","isLowTime","value","isVeryLowTime","formatTime","ms","oldMinutes","oldSeconds","oldMilliseconds","mins","Math","floor","secs","msds","toString","padStart","setTimeout","startCountdown","clearInterval","setInterval","countdown"],"sources":["/Users/ajun/projects/github/landing-page/lotteryDraw/nigeria/src/components/countdown/CircularCountdown.vue"],"sourcesContent":["<template>\n  <div \n    class=\"countdown-container relative overflow-hidden rounded-xl  from-orange-500 to-red-600 p-6 shadow-2xl transition-all duration-1000 md:p-8\"\n    :class=\"{ \n      'animate-pulse': isLowTime,\n      'from-red-600 to-red-800': isVeryLowTime \n    }\"\n  >\n    <!-- Ë£ÖÈ•∞ÂÖÉÁ¥† - ÈöèÊó∂Èó¥ÂèòÂåñ -->\n    <div \n      class=\"absolute -top-10 -right-10 h-40 w-40 rounded-full blur-3xl transition-all duration-1000\"\n      :class=\"{ 'bg-white/20': isLowTime, 'bg-white/10': !isLowTime }\"\n    ></div>\n    <div \n      class=\"absolute -bottom-10 -left-10 h-40 w-40 rounded-full blur-3xl transition-all duration-1000\"\n      :class=\"{ 'bg-white/20': isLowTime, 'bg-white/10': !isLowTime }\"\n    ></div>\n    \n    <!-- ÁÅ´ÁÑ∞Ë£ÖÈ•∞ - ÈöèÊó∂Èó¥Â¢ûÂº∫ -->\n    <div v-if=\"isLowTime\" class=\"absolute top-2 left-2 text-yellow-300 drop-shadow-lg\">\n      <i class=\"fa fa-fire text-xl animate-pulse\"></i>\n    </div>\n    <div v-if=\"isLowTime\" class=\"absolute top-2 right-2 text-yellow-300 drop-shadow-lg\">\n      <i class=\"fa fa-fire text-xl animate-pulse\"></i>\n    </div>\n    \n    <!-- Ê†áÈ¢ò - ÈöèÊó∂Èó¥ÂèòÂåñ -->\n    <h2 \n      class=\"mb-6 text-center text-2xl font-bold text-white drop-shadow-md transition-all duration-500 md:text-3xl\"\n      :class=\"{ \n        'text-yellow-100 scale-105': isLowTime,\n        'text-yellow-300 scale-110': isVeryLowTime \n      }\"\n    >\n      {{ countdown.label }}\n      <span v-if=\"isVeryLowTime\" class=\"inline-block ml-2 animate-bounce\">‚è∞</span>\n    </h2>\n    \n    <!-- ÂÄíËÆ°Êó∂ÊòæÁ§∫ -->\n    <div class=\"flex flex-col items-center justify-center gap-6\">\n      <!-- Êó∂Èó¥Êï∞Â≠óÊòæÁ§∫ -->\n      <div class=\"flex flex-wrap items-center justify-center gap-3 md:gap-6\">\n        <!-- ÂàÜÈíü -->\n        <div class=\"time-box group relative\">\n          <div \n            class=\"time-digit h-24 w-20 overflow-hidden rounded-lg bg-white/10 backdrop-blur-md px-3 text-4xl font-bold text-white shadow-lg transition-all duration-300 md:h-32 md:w-28 md:text-6xl\"\n            :class=\"{ \n              'bg-red-500/30': isLowTime,\n              'bg-red-600/40 transform scale-105': isVeryLowTime\n            }\"\n          >\n            <span :class=\"{ 'animate-slide': minuteChanged }\" class=\"flex h-full w-full items-center justify-center\">\n              {{ minutes }}\n            </span>\n          </div>\n          <span class=\"absolute -bottom-6 left-1/2 -translate-x-1/2 whitespace-nowrap rounded-full bg-white/20 px-2 py-1 text-xs font-medium text-white transition-all duration-300 backdrop-blur-sm group-hover:bg-white/30\">\n            {{ countdown.labels.minute }}\n          </span>\n        </div>\n        \n        <div class=\"time-separator hidden items-center text-3xl font-bold text-white/70 md:flex\">\n          :\n        </div>\n        \n        <!-- ÁßíÈíü -->\n        <div class=\"time-box group relative\">\n          <div \n            class=\"time-digit h-24 w-20 overflow-hidden rounded-lg bg-white/10 backdrop-blur-md px-3 text-4xl font-bold text-white shadow-lg transition-all duration-300 md:h-32 md:w-28 md:text-6xl\"\n            :class=\"{ \n              'bg-red-500/30': isLowTime,\n              'bg-red-600/40 transform scale-105': isVeryLowTime\n            }\"\n          >\n            <span :class=\"{ 'animate-slide': secondChanged }\" class=\"flex h-full w-full items-center justify-center\">\n              {{ seconds }}\n            </span>\n          </div>\n          <span class=\"absolute -bottom-6 left-1/2 -translate-x-1/2 whitespace-nowrap rounded-full bg-white/20 px-2 py-1 text-xs font-medium text-white transition-all duration-300 backdrop-blur-sm group-hover:bg-white/30\">\n            {{ countdown.labels.second }}\n          </span>\n        </div>\n        \n        <div class=\"time-separator hidden items-center text-3xl font-bold text-white/70 md:flex\">\n          :\n        </div>\n        \n        <!-- ÊØ´Áßí -->\n        <div class=\"time-box group relative\">\n          <div \n            class=\"time-digit h-24 w-20 overflow-hidden rounded-lg bg-white/10 backdrop-blur-md px-3 text-4xl font-bold text-white shadow-lg transition-all duration-300 md:h-32 md:w-28 md:text-6xl\"\n            :class=\"{ \n              'bg-red-500/30': isLowTime,\n              'bg-red-600/40 transform scale-105': isVeryLowTime\n            }\"\n          >\n            <span :class=\"{ 'animate-slide': millisecondChanged }\" class=\"flex h-full w-full items-center justify-center\">\n              {{ milliseconds }}\n            </span>\n          </div>\n          <span class=\"absolute -bottom-6 left-1/2 -translate-x-1/2 whitespace-nowrap rounded-full bg-white/20 px-2 py-1 text-xs font-medium text-white transition-all duration-300 backdrop-blur-sm group-hover:bg-white/30\">\n            {{ countdown.labels.millisecond }}\n          </span>\n        </div>\n      </div>\n      \n      <!-- ÁßªÂä®Á´ØÊó∂Èó¥ÂàÜÈöîÁ¨¶ -->\n      <div class=\"flex items-center justify-center gap-8 text-lg font-bold text-white/70 md:hidden\">\n        <span>{{ countdown.labels.minute }}</span>\n        <span>{{ countdown.labels.second }}</span>\n        <span>{{ countdown.labels.millisecond }}</span>\n      </div>\n      \n      <!-- ËøõÂ∫¶Êù° - ÈöèÊó∂Èó¥ÂèòËâ≤ -->\n      <div class=\"mt-4 h-2 w-full max-w-md rounded-full bg-white/20 overflow-hidden shadow-inner\">\n        <div \n          :style=\"{ width: progress + '%' }\"\n          class=\"h-full transition-all duration-100 ease-linear\"\n          :class=\"{\n            'bg-green-400': progress > 60,\n            'bg-yellow-400': progress <= 60 && progress > 30,\n            'bg-orange-400': progress <= 30 && progress > 10,\n            'bg-red-400 animate-pulse': progress <= 10\n          }\"\n        ></div>\n      </div>\n      \n      <!-- ÂÄíËÆ°Êó∂ÁªìÊùüÊèêÁ§∫ -->\n      <div v-if=\"remainingTime <= 0\" class=\"mt-4 text-center\">\n        <p class=\"text-xl font-bold text-yellow-200 drop-shadow-lg animate-bounce\">Êó∂Èó¥Âà∞ÔºÅ</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted, onUnmounted, computed } from 'vue';\n\n// ÈÖçÁΩÆÂèÇÊï∞\nconst countdownConfig = {\n  label: 'üî• Ê¥ªÂä®ÁÅ´ÁÉ≠ËøõË°å‰∏≠ÔºÅÂÄíËÆ°Êó∂ üî•',\n  totalDuration: 60000, // ÊÄªÊó∂ÈïøÔºåÊØ´Áßí\n  labels: {\n    minute: 'ÂàÜ',\n    second: 'Áßí',\n    millisecond: 'ÊØ´Áßí'\n  }\n};\n\n// ÂÄíËÆ°Êó∂Áä∂ÊÄÅ\nconst remainingTime = ref(countdownConfig.totalDuration);\nconst minutes = ref('00');\nconst seconds = ref('00');\nconst milliseconds = ref('000');\nconst progress = ref(100);\nconst intervalId = ref(null);\n\n// Âä®ÁîªÁä∂ÊÄÅ\nconst minuteChanged = ref(false);\nconst secondChanged = ref(false);\nconst millisecondChanged = ref(false);\n\n// ËÆ°ÁÆó‰ΩéÊó∂Èó¥Áä∂ÊÄÅ (ÊúÄÂêé20Áßí)\nconst isLowTime = computed(() => {\n  return remainingTime.value <= 20000 && remainingTime.value > 0;\n});\n\n// ËÆ°ÁÆóÊûÅ‰ΩéÊó∂Èó¥Áä∂ÊÄÅ (ÊúÄÂêé10Áßí)\nconst isVeryLowTime = computed(() => {\n  return remainingTime.value <= 10000 && remainingTime.value > 0;\n});\n\n// Ê†ºÂºèÂåñÊó∂Èó¥\nconst formatTime = () => {\n  const ms = remainingTime.value;\n  \n  // ‰øùÂ≠òÊóßÂÄºÁî®‰∫éÊ£ÄÊµãÂèòÂåñ\n  const oldMinutes = minutes.value;\n  const oldSeconds = seconds.value;\n  const oldMilliseconds = milliseconds.value;\n  \n  // ËÆ°ÁÆóÂêÑÊó∂Èó¥Âçï‰Ωç\n  const mins = Math.floor(ms / 60000);\n  const secs = Math.floor((ms % 60000) / 1000);\n  const msds = Math.floor((ms % 1000));\n  \n  // Ê†ºÂºèÂåñÊòæÁ§∫ÔºàË°•Èõ∂Ôºâ\n  minutes.value = mins.toString().padStart(2, '0');\n  seconds.value = secs.toString().padStart(2, '0');\n  milliseconds.value = msds.toString().padStart(3, '0');\n  \n  // Êõ¥Êñ∞ËøõÂ∫¶Êù°\n  progress.value = (ms / countdownConfig.totalDuration) * 100;\n  \n  // Ê£ÄÊµãÂèòÂåñÂπ∂Ëß¶ÂèëÂä®Áîª\n  minuteChanged.value = oldMinutes !== minutes.value;\n  secondChanged.value = oldSeconds !== seconds.value;\n  millisecondChanged.value = oldMilliseconds !== milliseconds.value;\n  \n  // ÈáçÁΩÆÂä®ÁîªÁä∂ÊÄÅ\n  if (minuteChanged.value) {\n    setTimeout(() => minuteChanged.value = false, 500);\n  }\n  if (secondChanged.value) {\n    setTimeout(() => secondChanged.value = false, 500);\n  }\n  if (millisecondChanged.value) {\n    setTimeout(() => millisecondChanged.value = false, 500);\n  }\n};\n\n// ÂºÄÂßãÂÄíËÆ°Êó∂\nconst startCountdown = () => {\n  // Ê∏ÖÈô§ÂèØËÉΩÂ≠òÂú®ÁöÑÂÆöÊó∂Âô®\n  if (intervalId.value) {\n    clearInterval(intervalId.value);\n  }\n  \n  // ËÆæÁΩÆÂÆöÊó∂Âô®ÔºåÊØè10ÊØ´ÁßíÊõ¥Êñ∞‰∏ÄÊ¨°\n  intervalId.value = setInterval(() => {\n    remainingTime.value -= 10;\n    \n    if (remainingTime.value <= 0) {\n      remainingTime.value = 0;\n      clearInterval(intervalId.value);\n    }\n    \n    formatTime();\n  }, 10);\n};\n\n// ÁªÑ‰ª∂ÊåÇËΩΩÊó∂ÂàùÂßãÂåñÂπ∂ÂºÄÂßãÂÄíËÆ°Êó∂\nonMounted(() => {\n  formatTime();\n  startCountdown();\n});\n\n// ÁªÑ‰ª∂Âç∏ËΩΩÊó∂Ê∏ÖÈô§ÂÆöÊó∂Âô®\nonUnmounted(() => {\n  if (intervalId.value) {\n    clearInterval(intervalId.value);\n  }\n});\n\n// Êö¥Èú≤ÁªôÊ®°Êùø\nconst countdown = countdownConfig;\n</script>\n\n<style scoped>\n.countdown-container {\n  @apply relative max-w-3xl mx-auto;\n}\n\n/* Êï∞Â≠óÂèòÂåñÂä®Áîª */\n.animate-slide {\n  animation: slide-down 0.5s ease-out forwards;\n}\n\n@keyframes slide-down {\n  0% {\n    transform: translateY(-100%);\n    opacity: 0;\n  }\n  50% {\n    transform: translateY(0);\n    opacity: 1;\n  }\n}\n\n/* Êó∂Èó¥ÂàÜÈöîÁ¨¶Èó™ÁÉÅÂä®Áîª */\n.time-separator {\n  animation: blink 1s step-end infinite;\n}\n\n@keyframes blink {\n  from, to { opacity: 1; }\n  50% { opacity: 0.4; }\n}\n\n/* ÊÇ¨ÂÅúÊïàÊûú */\n.time-box:hover .time-digit {\n  transform: scale(1.05);\n  box-shadow: 0 10px 25px -5px rgba(0, 0, 0, 0.2);\n}\n\n/* ‰ΩéÊó∂Èó¥Áä∂ÊÄÅ‰∏ãÁöÑÂ¢ûÂº∫Âä®Áîª */\n@keyframes pulse-fast {\n  0%, 100% { opacity: 1; }\n  50% { opacity: 0.7; }\n}\n\n.animate-pulse-fast {\n  animation: pulse-fast 0.6s cubic-bezier(0.4, 0, 0.6, 1) infinite;\n}\n\n/* ÂÄíËÆ°Êó∂ÁªìÊùüÊó∂ÁöÑÂä®Áîª */\n@keyframes celebrate {\n  0% { transform: scale(1); }\n  50% { transform: scale(1.2); }\n  100% { transform: scale(1); }\n}\n\n.animate-celebrate {\n  animation: celebrate 0.5s ease-in-out infinite;\n}\n</style>\n"],"mappings":"AAuIA,SAASA,GAAG,EAAEC,SAAS,EAAEC,WAAW,EAAEC,QAAQ,QAAQ,KAAK;;AAE3D;;;;;;;;IACA,MAAMC,eAAe,GAAG;MACtBC,KAAK,EAAE,mBAAmB;MAC1BC,aAAa,EAAE,KAAK;MAAE;MACtBC,MAAM,EAAE;QACNC,MAAM,EAAE,GAAG;QACXC,MAAM,EAAE,GAAG;QACXC,WAAW,EAAE;MACf;IACF,CAAC;;IAED;IACA,MAAMC,aAAa,GAAGX,GAAG,CAACI,eAAe,CAACE,aAAa,CAAC;IACxD,MAAMM,OAAO,GAAGZ,GAAG,CAAC,IAAI,CAAC;IACzB,MAAMa,OAAO,GAAGb,GAAG,CAAC,IAAI,CAAC;IACzB,MAAMc,YAAY,GAAGd,GAAG,CAAC,KAAK,CAAC;IAC/B,MAAMe,QAAQ,GAAGf,GAAG,CAAC,GAAG,CAAC;IACzB,MAAMgB,UAAU,GAAGhB,GAAG,CAAC,IAAI,CAAC;;IAE5B;IACA,MAAMiB,aAAa,GAAGjB,GAAG,CAAC,KAAK,CAAC;IAChC,MAAMkB,aAAa,GAAGlB,GAAG,CAAC,KAAK,CAAC;IAChC,MAAMmB,kBAAkB,GAAGnB,GAAG,CAAC,KAAK,CAAC;;IAErC;IACA,MAAMoB,SAAS,GAAGjB,QAAQ,CAAC,MAAM;MAC/B,OAAOQ,aAAa,CAACU,KAAK,IAAI,KAAK,IAAIV,aAAa,CAACU,KAAK,GAAG,CAAC;IAChE,CAAC,CAAC;;IAEF;IACA,MAAMC,aAAa,GAAGnB,QAAQ,CAAC,MAAM;MACnC,OAAOQ,aAAa,CAACU,KAAK,IAAI,KAAK,IAAIV,aAAa,CAACU,KAAK,GAAG,CAAC;IAChE,CAAC,CAAC;;IAEF;IACA,MAAME,UAAU,GAAGA,CAAA,KAAM;MACvB,MAAMC,EAAE,GAAGb,aAAa,CAACU,KAAK;;MAE9B;MACA,MAAMI,UAAU,GAAGb,OAAO,CAACS,KAAK;MAChC,MAAMK,UAAU,GAAGb,OAAO,CAACQ,KAAK;MAChC,MAAMM,eAAe,GAAGb,YA