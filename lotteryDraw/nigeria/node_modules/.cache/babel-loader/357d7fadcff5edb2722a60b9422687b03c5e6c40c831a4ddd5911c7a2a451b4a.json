{"ast":null,"code":"import { ref, onMounted, onUnmounted } from 'vue';\nimport FlipDigit from './FlipDigit.vue'; // ÂºïÂÖ•ÁøªÈ°µÊï∞Â≠óÂ≠êÁªÑ‰ª∂\n\n// ÈÖçÁΩÆÂèÇÊï∞Ôºà‰∏éÈúÄÊ±Ç‰∏ÄËá¥Ôºâ\n\nexport default {\n  __name: 'FlipCountdown',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const countdownConfig = {\n      label: 'üî• Ê¥ªÂä®ÁÅ´ÁÉ≠ËøõË°å‰∏≠ÔºÅÂÄíËÆ°Êó∂ üî•',\n      totalDuration: 60000,\n      // ÊÄªÊó∂ÈïøÔºàÊØ´ÁßíÔºâ\n      labels: {\n        minute: 'ÂàÜ',\n        second: 'Áßí',\n        millisecond: 'ÊØ´Áßí'\n      }\n    };\n\n    // ÂÄíËÆ°Êó∂Áä∂ÊÄÅ\n    const remainingTime = ref(countdownConfig.totalDuration);\n    const minutes = ref('00');\n    const seconds = ref('00');\n    const milliseconds = ref('000');\n    let intervalId = null;\n\n    // Ê†ºÂºèÂåñÊó∂Èó¥ÔºàË°•Èõ∂ & ËÆ°ÁÆóÊó∂ÂàÜÁßíÔºâ\n    const formatTime = () => {\n      const ms = remainingTime.value;\n      const mins = Math.floor(ms / 60000);\n      const secs = Math.floor(ms % 60000 / 1000);\n      const msds = Math.floor(ms % 1000);\n      minutes.value = mins.toString().padStart(2, '0');\n      seconds.value = secs.toString().padStart(2, '0');\n      milliseconds.value = msds.toString().padStart(3, '0');\n      if (ms <= 0) {\n        remainingTime.value = 0;\n        clearInterval(intervalId);\n      }\n    };\n\n    // ÂêØÂä®ÂÄíËÆ°Êó∂\n    const startCountdown = () => {\n      if (intervalId) clearInterval(intervalId);\n      intervalId = setInterval(() => {\n        remainingTime.value -= 10; // Á≤æÂ∫¶Ôºö10ÊØ´Áßí\n        formatTime();\n      }, 10);\n    };\n    onMounted(() => {\n      formatTime();\n      startCountdown();\n    });\n    onUnmounted(() => {\n      clearInterval(intervalId);\n    });\n\n    // Êö¥Èú≤ÁªôÊ®°Êùø\n    const countdown = countdownConfig;\n    const __returned__ = {\n      countdownConfig,\n      remainingTime,\n      minutes,\n      seconds,\n      milliseconds,\n      get intervalId() {\n        return intervalId;\n      },\n      set intervalId(v) {\n        intervalId = v;\n      },\n      formatTime,\n      startCountdown,\n      countdown,\n      ref,\n      onMounted,\n      onUnmounted,\n      FlipDigit\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","FlipDigit","countdownConfig","label","totalDuration","labels","minute","second","millisecond","remainingTime","minutes","seconds","milliseconds","intervalId","formatTime","ms","value","mins","Math","floor","secs","msds","toString","padStart","clearInterval","startCountdown","setInterval","countdown"],"sources":["/Users/ajun/projects/github/landing-page/lotteryDraw/nigeria/src/components/countdown/FlipCountdown.vue"],"sourcesContent":["<template>\n  <div class=\"flip-clock-container relative mx-auto max-w-xl\">\n    <!-- Êú®Á∫πËÉåÊôØÔºàÊ®°ÊãüÂõæÁâáË¥®ÊÑüÔºâ -->\n    <div class=\"absolute inset-0 bg-cover bg-center\" style=\"background-image: url('https://picsum.photos/id/129/800/400'); filter: brightness(0.8);\"></div>\n    \n    <!-- ÈáëÂ±ûËæπÊ°ÜÁöÑÁøªÈ°µÊó∂ÈíüÂÆπÂô® -->\n    <div class=\"flip-clock relative flex items-center justify-center gap-3 rounded-2xl border-4 border-gray-700 bg-black/20 p-3 shadow-2xl backdrop-blur-sm\">\n      <!-- ÂàÜÈíüÁøªÈ°µÂô® -->\n      <FlipDigit \n        :value=\"minutes\" \n        :label=\"countdown.labels.minute\" \n        @change=\"minuteChanged = true\"\n        @animation-end=\"minuteChanged = false\"\n      />\n      \n      <!-- ÂàÜÈöîÁ¨¶ -->\n      <div class=\"text-3xl font-bold text-white/80\">:</div>\n      \n      <!-- ÁßíÈíüÁøªÈ°µÂô® -->\n      <FlipDigit \n        :value=\"seconds\" \n        :label=\"countdown.labels.second\"\n        @change=\"secondChanged = true\"\n        @animation-end=\"secondChanged = false\"\n      />\n      \n      <!-- ÂàÜÈöîÁ¨¶ -->\n      <div class=\"text-3xl font-bold text-white/80\">:</div>\n      \n      <!-- ÊØ´ÁßíÁøªÈ°µÂô® -->\n      <FlipDigit \n        :value=\"milliseconds\" \n        :label=\"countdown.labels.millisecond\"\n        @change=\"millisecondChanged = true\"\n        @animation-end=\"millisecondChanged = false\"\n      />\n    </div>\n    \n    <!-- ÂÄíËÆ°Êó∂Ê†áÈ¢ò -->\n    <h2 class=\"mt-6 text-center text-xl sm:text-2xl font-bold text-white drop-shadow-md\">\n      {{ countdown.label }}\n    </h2>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted, onUnmounted } from 'vue';\nimport FlipDigit from './FlipDigit.vue'; // ÂºïÂÖ•ÁøªÈ°µÊï∞Â≠óÂ≠êÁªÑ‰ª∂\n\n// ÈÖçÁΩÆÂèÇÊï∞Ôºà‰∏éÈúÄÊ±Ç‰∏ÄËá¥Ôºâ\nconst countdownConfig = {\n  label: 'üî• Ê¥ªÂä®ÁÅ´ÁÉ≠ËøõË°å‰∏≠ÔºÅÂÄíËÆ°Êó∂ üî•',\n  totalDuration: 60000, // ÊÄªÊó∂ÈïøÔºàÊØ´ÁßíÔºâ\n  labels: {\n    minute: 'ÂàÜ',\n    second: 'Áßí',\n    millisecond: 'ÊØ´Áßí'\n  }\n};\n\n// ÂÄíËÆ°Êó∂Áä∂ÊÄÅ\nconst remainingTime = ref(countdownConfig.totalDuration);\nconst minutes = ref('00');\nconst seconds = ref('00');\nconst milliseconds = ref('000');\nlet intervalId = null;\n\n// Ê†ºÂºèÂåñÊó∂Èó¥ÔºàË°•Èõ∂ & ËÆ°ÁÆóÊó∂ÂàÜÁßíÔºâ\nconst formatTime = () => {\n  const ms = remainingTime.value;\n  const mins = Math.floor(ms / 60000);\n  const secs = Math.floor((ms % 60000) / 1000);\n  const msds = Math.floor((ms % 1000));\n  \n  minutes.value = mins.toString().padStart(2, '0');\n  seconds.value = secs.toString().padStart(2, '0');\n  milliseconds.value = msds.toString().padStart(3, '0');\n  \n  if (ms <= 0) {\n    remainingTime.value = 0;\n    clearInterval(intervalId);\n  }\n};\n\n// ÂêØÂä®ÂÄíËÆ°Êó∂\nconst startCountdown = () => {\n  if (intervalId) clearInterval(intervalId);\n  intervalId = setInterval(() => {\n    remainingTime.value -= 10; // Á≤æÂ∫¶Ôºö10ÊØ´Áßí\n    formatTime();\n  }, 10);\n};\n\nonMounted(() => {\n  formatTime();\n  startCountdown();\n});\n\nonUnmounted(() => {\n  clearInterval(intervalId);\n});\n\n// Êö¥Èú≤ÁªôÊ®°Êùø\nconst countdown = countdownConfig;\n</script>\n\n<style scoped>\n.f