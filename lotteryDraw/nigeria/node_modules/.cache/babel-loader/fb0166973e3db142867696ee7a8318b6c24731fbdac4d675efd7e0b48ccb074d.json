\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted, onUnmounted, watch } from 'vue';\n\nconst props = defineProps({\n  label: { type: String, required: true },\n  totalDuration: { type: Number, required: true },\n  labels: { type: Object, required: true }\n});\n\nconst minutes = ref(0);\nconst seconds = ref(0);\nconst milliseconds = ref(0);\n\nlet startTime, timer;\nlet prevMinutes = 0;\nlet prevSeconds = 0;\nlet prevMilliseconds = 0;\n\nconst updateCountdown = () => {\n  const now = new Date().getTime();\n  const elapsed = now - startTime;\n  const remaining = Math.max(props.totalDuration - elapsed, 0);\n\n  const newMinutes = Math.floor((remaining / 1000 / 60) % 60);\n  const newSeconds = Math.floor((remaining / 1000) % 60);\n  const newMilliseconds = Math.floor(remaining % 1000);\n\n  if (newMinutes !== prevMinutes) {\n    minutes.value = newMinutes;\n    prevMinutes = newMinutes;\n    document.querySelector('.minutes')?.classList.add('number-change');\n    setTimeout(() => {\n      document.querySelector('.minutes')?.classList.remove('number-change');\n    }, 500);\n  }\n\n  if (newSeconds !== prevSeconds) {\n    seconds.value = newSeconds;\n    prevSeconds = newSeconds;\n    document.querySelector('.seconds')?.classList.add('number-change');\n    setTimeout(() => {\n      document.querySelector('.seconds')?.classList.remove('number-change');\n    }, 500);\n  }\n\n  if (newMilliseconds !== prevMilliseconds) {\n    milliseconds.value = newMilliseconds;\n    prevMilliseconds = newMilliseconds;\n    document.querySelector('.milliseconds')?.classList.add('number-change');\n    setTimeout(() => {\n      document.querySelector('.milliseconds')?.classList.remove('number-change');\n    }, 100);\n  }\n\n  if (remaining <= 0) clearInterval(timer);\n};\n\nonMounted(() => {\n  startTime = new Date().getTime();\n  timer = setInterval(updateCountdown, 50);\n  prevMinutes = minutes.value;\n  prevSeconds = seconds.value;\n  prevMilliseconds = milliseconds.value;\n});\n\nonUnmounted(() => {\n  clearInterval(timer);\n});\n</script>\n\n<style scoped>\n/* 全局变量 */\n:root {\n  --primary-color: #ff3e6c;\n  --secondary-color: #0070f3;\n  --accent-color: #ffbb00;\n  --bg-dark: #111827;\n  --bg-light: #1f2937;\n  --text-primary: #ffffff;\n  --text-secondary: #9ca3af;\n  --shadow-light: rgba(255, 255, 255, 0.1);\n  --shadow-dark: rgba(0, 0, 0, 0.5);\n}\n\n/* 主容器 */\n.countdown-container {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  gap: 1.5rem;\n  padding: 2rem;\n  border-radius: 1rem;\n  background: var(--bg-dark);\n  box-shadow: \n    0 10px 30px var(--shadow-dark),\n    0 0 0 1px var(--shadow-light);\n  position: relative;\n  overflow: hidden;\n}\n\n/* 背景装饰 */\n.countdown-container::before {\n  content: '';\n  position: absolute;\n  top: -50%;\n  left: -50%;\n  width: 200%;\n  height: 200%;\n  background: radial-gradient(circle at center, var(--shadow-light) 0%, transparent 70%);\n  pointer-events: none;\n}\n\n/* 倒计时标题 */\n.countdown-title {\n  font-size: 1.8rem;\n  font-weight: 700;\n  color: var(--text-primary);\n  text-align: center;\n  letter-spacing: 0.05em;\n  margin: 0;\n  position: relative;\n  background: linear-gradient(90deg, var(--primary-color), var(--secondary-color));\n  background-size: 200% auto;\n  -webkit-background-clip: text;\n  background-clip: text;\n  color: transparent;\n  -webkit-text-fill-color: transparent;\n  animation: title-gradient 3s ease infinite;\n}\n\n@keyframes title-gradient {\n  0% {\n    background-position: 0% center;\n  }\n  50% {\n    background-position: 100% center;\n  }\n  100% {\n    background-position: 0% center;\n  }\n}\n\n/* 倒计时显示区域 */\n.countdown-display {\n  display: flex;\n  align-items: flex-end;\n  gap: 0.5rem;\n  position: relative;\n}\n\n/* 数字块容器 */\n.countdown-block {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  position: relative;\n}\n\n/* 毫秒块特殊样式 */\n.millisecond-block {\n  margin-left: 0.5rem;\n}\n\n/* 数字显示 */\n.countdown-number {\n  min-width: 3.5rem;\n  height: 5rem;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: 2.5rem;\n  font-weight: 800;\n  color: var(--text-primary);\n  background: var(--bg-light);\n  border-radius: 0.5rem;\n  border: 1px solid var(--shadow-light);\n  position: relative;\n  overflow: hidden;\n  box-shadow: \n    0 4px 8px var(--shadow-dark),\n    0 0 0 1px var(--shadow-light) inset;\n  transition: all 0.3s ease;\n}\n\n/* 毫秒数字特殊样式 */\n.countdown-number.milliseconds {\n  min-width: 4.5rem;\n  font-size: 2rem;\n  background: linear-gradient(135deg, rgba(0, 112, 243, 0.1) 0%, rgba(0, 112, 243, 0.05) 100%);\n  border: 1px solid rgba(0, 112, 243, 0.3);\n}\n\n/* 分钟数字特殊样式 */\n.countdown-number.minutes {\n  background: linear-gradient(135deg, rgba(255, 187, 0, 0.1) 0%, rgba(255, 187, 0, 0.05) 100%);\n  border: 1px solid rgba(255, 187, 0, 0.3);\n}\n\n/* 秒数字特殊样式 */\n.countdown-number.seconds {\n  background: linear-gradient(135deg, rgba(255, 62, 108, 0.1) 0%, rgba(255, 62, 108, 0.05) 100%);\n  border: 1px solid rgba(255, 62, 108, 0.3);\n}\n\n/* 数字变化动画 */\n.number-change {\n  animation: number-flip 0.5s cubic-bezier(0.34, 1.56, 0.64, 1);\n}\n\n@keyframes number-flip {\n  0% {\n    transform: scaleY(1);\n  }\n  50% {\n    transform: scaleY(0.8) rotateX(-15deg);\n  }\n  100% {\n    transform: scaleY(1);\n  }\n}\n\n/* 数字内部装饰 */\n.countdown-number::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  height: 1px;\n  background: linear-gradient(90deg, transparent, var(--shadow-light), transparent);\n}\n\n.countdown-number::after {\n  content: '';\n  position: absolute;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  height: 1px;\n  background: linear-gradient(90deg, transparent, var(--shadow-light), transparent);\n}\n\n/* 单位标签 */\n.countdown-label {\n  font-size: 0.7rem;\n  font-weight: 600;\n  color: var(--text-secondary);\n  text-transform: uppercase;\n  letter-spacing: 0.1em;\n  margin-top: 0.5rem;\n  text-align: center;\n}\n\n/* 分隔符 */\n.countdown-separator {\n  font-size: 2rem;\n  font-weight: 300;\n  color: var(--accent-color);\n  line-height: 1;\n  padding-bottom: 0.5rem;\n  text-shadow: 0 0 10px rgba(255, 187, 0, 0.3);\n  animation: separator-pulse 1s ease-in-out infinite alternate;\n}\n\n@keyframes separator-pulse {\n  from {\n    opacity: 0.7;\n    transform: scale(0.95);\n  }\n  to {\n    opacity: 1;\n    transform: scale(1.05);\n  }\n}\n\n/* 毫秒数字额外效果 */\n.countdown-number.milliseconds {\n  animation: millisecond-glow 1s ease-in-out infinite alternate;\n}\n\n@keyframes millisecond-glow {\n  from {\n    box-shadow: \n      0 4px 8px var(--shadow-dark),\n      0 0 0 1px var(--shadow-light) inset,\n      0 0 5px rgba(0, 112, 243, 0.3);\n  }\n  to {\n    box-shadow: \n      0 4px 8px var(--shadow-dark),\n      0 0 0 1px var(--shadow-light) inset,\n      0 0 15px rgba(0, 112, 243, 0.6),\n      0 0 25px rgba(0, 112, 243, 0.3);\n  }\n}\n\n/* 分钟数字额外效果 */\n.countdown-number.minutes {\n  animation: minute-glow 2s ease-in-out infinite alternate;\n}\n\n@keyframes minute-glow {\n  from {\n    box-shadow: \n      0 4px 8px var(--shadow-dark),\n      0 0 0 1px var(--shadow-light) inset,\n      0 0 5px rgba(255, 187, 0, 0.3);\n  }\n  to {\n    box-shadow: \n      0 4px 8px var(--shadow-dark),\n      0 0 0 1px var(--shadow-light) inset,\n      0 0 12px rgba(255, 187, 0, 0.5),\n      0 0 20px rgba(255, 187, 0, 0.3);\n  }\n}\n\n/* 秒数字额外效果 */\n.countdown-number.seconds {\n  animation: second-glow 1.5s ease-in-out infinite alternate;\n}\n\n@keyframes second-glow {\n  from {\n    box-shadow: \n      0 4px 8px var(--shadow-dark),\n      0 0 0 1px var(--shadow-light) inset,\n      0 0 5px rgba(255, 62, 108, 0.3);\n  }\n  to {\n    box-shadow: \n      0 4px 8px var(--shadow-dark),\n      0 0 0 1px var(--shadow-light) inset,\n      0 0 12px rgba(255, 62, 108, 0.5),\n      0 0 20px rgba(255, 62, 108, 0.3);\n  }\n}\n\n/* 响应式设计 - 平板 */\n@media (max-width: 768px) {\n  .countdown-container {\n    padding: 1.5rem;\n    gap: 1rem;\n  }\n  \n  .countdown-title {\n    font-size: 1.5rem;\n  }\n  \n  .countdown-number {\n    min-width: 3rem;\n    height: 4rem;\n    font-size: 2rem;\n  }\n  \n  .countdown-number.milliseconds {\n    min-width: 3.8rem;\n    font-size: 1.6rem;\n  }\n  \n  .countdown-separator {\n    font-size: 1.5rem;\n  }\n  \n  .countdown-label {\n    font-size: 0.6rem;\n    margin-top: 0.3rem;\n  }\n}\n\n/* 响应式设计 - 手机 */\n@media (max-width: 480px) {\n  .countdown-container {\n    padding: 1rem;\n  }\n  \n  .countdown-title {\n    font-size: 1.2rem;\n  }\n  \n  .countdown-number {\n    min-width: 2.5rem;\n    height: 3.5rem;\n    font-size: 1.6rem;\n  }\n  \n  .countdown-number.milliseconds {\n    min-width: 3.2rem;\n    font-size: 1.3rem;\n  }\n  \n  .countdown-separator {\n    font-size: 1.2rem;\n  }\n  \n  .countdown-label {\n    font-size: 0.5rem;\n  }\n}\n</style>\n"],"mappings":";;EACOA,KAAK,EAAC;AAAqB;;EAE1BA,KAAK,EAAC;AAAiB;;EAKtBA,KAAK,EAAC;AAAmB;;EAEvBA,KAAK,EAAC;AAAiB;;EACrBA,KAAK,EAAC;AAA0B;;EAChCA,KAAK,EAAC;AAAiB;;EAMzBA,KAAK,EAAC;AAAiB;;EACrBA,KAAK,EAAC;AAA0B;;EAChCA,KAAK,EAAC;AAAiB;;EAMzBA,KAAK,EAAC;AAAmC;;EACvCA,KAAK,EAAC;AAA+B;;EACrCA,KAAK,EAAC;AAAiB;;uBA3BlCC,mBAAA,CA8BM,OA9BNC,UA8BM,GA7BJC,mBAAA,QAAW,EACXC,mBAAA,CAEK,MAFLC,UAEK,EAAAC,gBAAA,CADAC,MAAA,CAAAC,KAAK,kBAGVL,mBAAA,WAAc,EACdC,mBAAA,CAsBM,OAtBNK,UAsBM,GArBJN,mBAAA,QAAW,EACXC,mBAAA,CAGM,OAHNM,UAGM,GAFJN,mBAAA,CAAqF,OAArFO,UAAqF,EAAAL,gBAAA,CAA5CM,MAAA,CAAAC,OAAO,CAACC,QAAQ,GAAGC,QAAQ,0BACpEX,mBAAA,CAAsD,OAAtDY,UAAsD,EAAAV,gBAAA,CAAtBC,MAAA,CAAAU,MAAM,CAACC,MAAM,iB,6BAG/Cd,mBAAA,CAAwC;IAAnCJ,KAAK,EAAC;EAAqB,GAAC,GAAC,qBAElCG,mBAAA,OAAU,EACVC,mBAAA,CAGM,OAHNe,UAGM,GAFJf,mBAAA,CAAqF,OAArFgB,UAAqF,EAAAd,gBAAA,CAA5CM,MAAA,CAAAS,OAAO,CAACP,QAAQ,GAAGC,QAAQ,0BACpEX,mBAAA,CAAsD,OAAtDkB,UAAsD,EAAAhB,gBAAA,CAAtBC,MAAA,CAAAU,MAAM,CAACM,MAAM,iB,6BAG/CnB,mBAAA,CAAwC;IAAnCJ,KAAK,EAAC;EAAqB,GAAC,GAAC,qBAElCG,mBAAA,QAAW,EACXC,mBAAA,CAGM,OAHNoB,WAGM,GAFJp