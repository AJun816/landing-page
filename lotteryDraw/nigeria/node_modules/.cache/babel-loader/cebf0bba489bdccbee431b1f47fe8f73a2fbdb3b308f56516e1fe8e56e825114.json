own","now","Date","remaining","Math","max","value","floor","clearInterval","countdownConfig","totalSeconds","setInterval"],"sources":["/Users/ajun/projects/github/landing-page/lotteryDraw/nigeria/src/components/countdown/Countdown3D.vue"],"sourcesContent":["<template>\n  <div class=\"flex flex-col items-center text-white space-y-6\">\n    <!-- 倒计时标题 -->\n    <p class=\"text-lg md:text-2xl font-bold animate-pulse\">\n      {{ countdownConfig.label }}\n    </p>\n\n    <!-- 倒计时展示 -->\n    <div class=\"flex items-center space-x-3 md:space-x-6\">\n      <!-- 天 -->\n      <div v-if=\"countdownConfig.showDays\" class=\"flex flex-col items-center\">\n        <span class=\"digit\">\n          {{ days.toString().padStart(2, '0') }}\n        </span>\n        <span class=\"text-sm mt-1 opacity-80\">{{ countdownConfig.labels.day }}</span>\n      </div>\n\n      <!-- 时 -->\n      <div class=\"flex flex-col items-center\">\n        <span class=\"digit\">\n          {{ hours.toString().padStart(2, '0') }}\n        </span>\n        <span class=\"text-sm mt-1 opacity-80\">{{ countdownConfig.labels.hour }}</span>\n      </div>\n\n      <!-- 分 -->\n      <div class=\"flex flex-col items-center\">\n        <span class=\"digit\">\n          {{ minutes.toString().padStart(2, '0') }}\n        </span>\n        <span class=\"text-sm mt-1 opacity-80\">{{ countdownConfig.labels.minute }}</span>\n      </div>\n\n      <!-- 秒 -->\n      <div class=\"flex flex-col items-center\">\n        <span class=\"digit\">\n          {{ seconds.toString().padStart(2, '0') }}\n        </span>\n        <span class=\"text-sm mt-1 opacity-80\">{{ countdownConfig.labels.second }}</span>\n      </div>\n\n      <!-- 毫秒 -->\n      <div class=\"flex flex-col items-center\">\n        <span class=\"digit text-pink-400\">\n          {{ milliseconds.toString().padStart(3, '0') }}\n        </span>\n        <span class=\"text-sm mt-1 opacity-80\">ms</span>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted, onUnmounted } from 'vue';\n\n// 传入完整配置\nconst props = defineProps({\n  countdownConfig: {\n    type: Object,\n    required: true\n  }\n});\n\nconst days = ref(0);\nconst hours = ref(0);\nconst minutes = ref(0);\nconst seconds = ref(0);\nconst milliseconds = ref(0);\n\nlet timer = null;\nlet endTime = null;\n\nconst updateCountdown = () => {\n  const now = Date.now();\n  const remaining = Math.max(endTime - now, 0);\n\n  days.value = Math.floor(remaining / (1000 * 60 * 60 * 24));\n  hours.value = Math.floor((remaining / (1000 * 60 * 60)) % 24);\n  minutes.value = Math.floor((remaining / (1000 * 60)) % 60);\n  seconds.value = Math.floor((remaining / 1000) % 60);\n  milliseconds.value = remaining % 1000;\n\n  if (remaining <= 0) {\n    clearInterval(timer);\n  }\n};\n\nonMounted(() => {\n  endTime = Date.now() + props.countdownConfig.totalSeconds * 1000;\n  updateCountdown();\n  timer = setInterval(updateCountdown, 50);\n});\n\nonUnmounted(() => {\n  if (timer) clearInterval(timer);\n});\n</script>\n\n<style scoped>\n.digit {\n  @apply text-3xl md:text-5xl font-extrabold px-4 py-2 rounded-lg shadow-xl;\n  background: linear-gradient(145deg, #ff0066, #6600ff);\n  color: white;\n  text-shadow: 0 0 10px #ff00ff, 0 0 20px #00ffff;\n  transform: perspective(400px) rotateX(5deg);\n  animation: flip 1s infinite alternate;\n}\n@keyframes flip {\n  0% { transform: perspective(400px) rotateX(5deg); }\n  100% { transform: perspective(400px) rotateX(-5deg); }\n}\n</style>\n"],"mappings":"AAqDA,SAASA,GAAG,EAAEC,SAAS,EAAEC,WAAW,QAAQ,KAAK;;AAEjD;;;;;;;;;;;;;;IACA,MAAMC,KAAK,GAAGC,OAKZ;IAEF,MAAMC,IAAI,GAAGL,GAAG,CAAC,CAAC,CAAC;IACnB,MAAMM,KAAK,GAAGN,GAAG,CAAC,CAAC,CAAC;IACpB,MAAMO,OAAO,GAAGP,GAAG,CAAC,CAAC,CAAC;IACtB,MAAMQ,OAAO,GAAGR,GAAG,CAAC,CAAC,CAAC;IACtB,MAAMS,YAAY,GAAGT,GAAG,CAAC,CAAC,CAAC;IAE3B,IAAIU,KAAK,GAAG,IAAI;IAChB,IAAIC,OAAO,GAAG,IAAI;IAElB,MAAMC,eAAe,GAAGA,CAAA,KAAM;MAC5B,MAAMC,GAAG,GAAGC,IAAI,CAACD,GAAG,CAAC,CAAC;MACtB,MAAME,SAAS,GAAGC,IAAI,CA