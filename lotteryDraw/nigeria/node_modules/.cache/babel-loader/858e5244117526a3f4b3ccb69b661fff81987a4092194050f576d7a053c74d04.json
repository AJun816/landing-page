,"_hoisted_11","_hoisted_12","_hoisted_13","seconds","_hoisted_14","_hoisted_15","second","_hoisted_16","_hoisted_17","_hoisted_18","_hoisted_19","milliseconds","_hoisted_20","_hoisted_21","millisecond"],"sources":["/Users/ajun/projects/github/landing-page/lotteryDraw/nigeria/src/components/countdown/Countdown3D.vue"],"sourcesContent":["<template>\n  <div class=\"flex flex-col items-center text-white space-y-6\">\n    <!-- 标题 -->\n    <p class=\"text-2xl md:text-3xl font-extrabold animate-pulse tracking-widest drop-shadow-glow\">\n      {{ label }}\n    </p>\n\n    <!-- 倒计时容器 -->\n    <div class=\"flex items-center space-x-3 md:space-x-6\">\n      <!-- 分钟 -->\n      <div class=\"flip-box\">\n        <div class=\"flip-card\">\n          <div class=\"flip-card-inner\">\n            <div class=\"flip-card-front minute-front\">{{ minutes.toString().padStart(2, '0') }}</div>\n            <div class=\"flip-card-back minute-back\">{{ minutes.toString().padStart(2, '0') }}</div>\n          </div>\n        </div>\n        <span class=\"flip-label\">{{ labels.minute }}</span>\n      </div>\n\n      <span class=\"separator\">:</span>\n\n      <!-- 秒 -->\n      <div class=\"flip-box\">\n        <div class=\"flip-card\">\n          <div class=\"flip-card-inner\">\n            <div class=\"flip-card-front second-front\">{{ seconds.toString().padStart(2, '0') }}</div>\n            <div class=\"flip-card-back second-back\">{{ seconds.toString().padStart(2, '0') }}</div>\n          </div>\n        </div>\n        <span class=\"flip-label\">{{ labels.second }}</span>\n      </div>\n\n      <span class=\"separator\">:</span>\n\n      <!-- 毫秒 -->\n      <div class=\"flip-box\">\n        <div class=\"flip-card millisecond\">\n          <div class=\"flip-card-inner\">\n            <div class=\"flip-card-front millisecond-front\">{{ milliseconds.toString().padStart(3, '0') }}</div>\n            <div class=\"flip-card-back millisecond-back\">{{ milliseconds.toString().padStart(3, '0') }}</div>\n          </div>\n        </div>\n        <span class=\"flip-label\">{{ labels.millisecond }}</span>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted, onUnmounted, watch } from 'vue';\n\nconst props = defineProps({\n  label: { type: String, required: true },\n  totalDuration: { type: Number, required: true },\n  labels: { type: Object, required: true }\n});\n\nconst minutes = ref(0);\nconst seconds = ref(0);\nconst milliseconds = ref(0);\n\nlet startTime, timer;\nlet prevMinutes = 0;\nlet prevSeconds = 0;\nlet prevMilliseconds = 0;\n\nconst updateCountdown = () => {\n  const now = new Date().getTime();\n  const elapsed = now - startTime;\n  const remaining = Math.max(props.totalDuration - elapsed, 0);\n\n  const newMinutes = Math.floor((remaining / 1000 / 60) % 60);\n  const newSeconds = Math.floor((remaining / 1000) % 60);\n  const newMilliseconds = Math.floor(remaining % 1000);\n\n  if (newMinutes !== prevMinutes) {\n    minutes.value = newMinutes;\n    prevMinutes = newMinutes;\n    document.querySelector('.minute-front')?.classList.add('flipping');\n    setTimeout(() => {\n      document.querySelector('.minute-front')?.classList.remove('flipping');\n    }, 500);\n  }\n\n  if (newSeconds !== prevSeconds) {\n    seconds.value = newSeconds;\n    prevSeconds = newSeconds;\n    document.querySelector('.second-front')?.classList.add('flipping');\n    setTimeout(() => {\n      document.querySelector('.second-front')?.classList.remove('flipping');\n    }, 500);\n  }\n\n  if (newMilliseconds !== prevMilliseconds) {\n    milliseconds.value = newMilliseconds;\n    prevMilliseconds = newMilliseconds;\n    document.querySelector('.millisecond-front')?.classList.add('flipping');\n    setTimeout(() => {\n      document.querySelector('.millisecond-front')?.classList.remove('flipping');\n    }, 100);\n  }\n\n  if (remaining <= 0) clearInterval(timer);\n};\n\nonMounted(() => {\n  startTime = new Date().getTime();\n  timer = setInterval(updateCountdown, 50);\n  prevMinutes = minutes.value;\n  prevSeconds = seconds.value;\n  prevMilliseconds = milliseconds.value;\n});\n\nonUnmounted(() => {\n  clearInterval(timer);\n});\n</script>\n\n<style scoped>\n/* 全局动画变量 */\n:root {\n  --glow-primary: #ff3366;\n  --glow-secondary: #66ccff;\n  --glow-accent: #ffcc00;\n}\n\n/* 标题发光 */\n.drop-shadow-glow {\n  text-shadow: 0 0 8px var(--glow-primary), 0 0 15px var(--glow-secondary), 0 0 25px var(--glow-primary);\n  animation: glowPulse 2s ease-in-out infinite alternate, float 3s ease-in-out infinite;\n  background: linear-gradient(90deg, #ff3366, #66ccff, #ffcc00);\n  background-size: 200% auto;\n  -webkit-background-clip: text;\n  background-clip: text;\n  color: transparent;\n  -webkit-text-fill-color: transparent;\n  animation: \n    glowPulse 2s ease-in-out infinite alternate,\n    float 3s ease-in-out infinite,\n    gradientFlow 5s ease infinite;\n}\n\n@keyframes glowPulse {\n  from {\n    text-shadow: 0 0 5px var(--glow-primary), 0 0 10px var(--glow-secondary), 0 0 15px var(--glow-primary);\n  }\n  to {\n    text-shadow: 0 0 15px var(--glow-primary), 0 0 25px var(--glow-secondary), 0 0 35px var(--glow-primary);\n  }\n}\n\n@keyframes float {\n  0%, 100% {\n    transform: translateY(0px);\n  }\n  50% {\n    transform: translateY(-8px);\n  }\n}\n\n@keyframes gradientFlow {\n  0% {\n    background-position: 0% center;\n  }\n  100% {\n    background-position: 200% center;\n  }\n}\n\n/* 翻牌容器 */\n.flip-box {\n  @apply flex flex-col items-center;\n  position: relative;\n}\n\n/* 翻牌卡片整体 */\n.flip-card {\n  width: 80px;\n  height: 120px;\n  perspective: 1500px;\n  position: relative;\n}\n\n/* 添加卡片装饰圈 */\n.flip-card::before {\n  content: '';\n  position: absolute;\n  top: -8px;\n  left: -8px;\n  right: -8px;\n  bottom: -8px;\n  border-radius: 12px;\n  background: radial-gradient(circle at center, rgba(255,255,255,0.2) 0%, rgba(255,255,255,0) 70%);\n  pointer-events: none;\n  z-index: 1;\n}\n\n/* 翻牌卡片内部（3D 效果） */\n.flip-card-inner {\n  position: relative;\n  width: 100%;\n  height: 100%;\n  text-align: center;\n  transition: transform 0.6s cubic-bezier(0.34, 1.56, 0.64, 1);\n  transform-style: preserve-3d;\n  box-shadow: 0 8px 25px rgba(0, 0, 0, 0.4), 0 0 20px rgba(255, 255, 255, 0.1) inset;\n  border-radius: 8px;\n}\n\n/* 翻转动画触发 */\n.flipping {\n  animation: flipAnimation 0.6s cubic-bezier(0.34, 1.56, 0.64, 1) forwards;\n  transform-origin: center;\n}\n\n@keyframes flipAnimation {\n  0% {\n    transform: rotateX(0deg);\n  }\n  50% {\n    transform: rotateX(90deg);\n    opacity: 0;\n  }\n  51% {\n    transform: rotateX(-90deg);\n    opacity: 0;\n  }\n  100% {\n    transform: rotateX(0deg);\n    opacity: 1;\n  }\n}\n\n/* 卡片正反两面 */\n.flip-card-front, .flip-card-back {\n  position: absolute;\n  width: 100%;\n  height: 100%;\n  backface-visibility: hidden;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: 2.5rem;\n  font-weight: 800;\n  border-radius: 8px;\n  overflow: hidden;\n  border: 1px solid rgba(255, 255, 255, 0.1);\n  position: relative;\n}\n\n/* 卡片装饰细节 */\n.flip-card-front::before, .flip-card-back::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  height: 2px;\n  background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.5), transparent);\n}\n\n.flip-card-front::after, .flip-card-back::after {\n  content: '';\n  position: absolute;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  height: 2px;\n  background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.5), transparent);\n}\n\n/* 分钟卡片特殊样式 */\n.minute-front, .minute-back {\n  background: linear-gradient(135deg, #ffcc00 0%, #ff9900 100%);\n  color: #333;\n  text-shadow: 0 2px 4px rgba(0, 0, 0, 0.2), 0 0 10px rgba(255, 255, 255, 0.5);\n  box-shadow: \n    0 0 15px rgba(255, 204, 0, 0.5),\n    0 5px 15px rgba(0, 0, 0, 0.3),\n    0 0 30px rgba(255, 153, 0, 0.3) inset;\n  animation: minuteGlow 3s ease-in-out infinite alternate;\n}\n\n@keyframes minuteGlow {\n  from {\n    box-shadow: \n      0 0 10px rgba(255, 204, 0, 0.5),\n      0 5px 15px rgba(0, 0, 0, 0.3),\n      0 0 20px rgba(255, 153, 0, 0.3) inset;\n  }\n  to {\n    box-shadow: \n      0 0 20px rgba(255, 204, 0, 0.8),\n      0 5px 20px rgba(0, 0, 0, 0.4),\n      0 0 40px rgba(255, 153, 0, 0.5) inset;\n  }\n}\n\n/* 秒卡片特殊样式 */\n.second-front, .second-back {\n  background: linear-gradient(135deg, #ff3366 0%, #ff0033 100%);\n  color: white;\n  text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3), 0 0 10px rgba(255, 255, 255, 0.5);\n  box-shadow: \n    0 0 15px rgba(255, 51, 102, 0.5),\n    0 5px 15px rgba(0, 0, 0, 0.3),\n    0 0 30px rgba(255, 0, 51, 0.3) inset;\n  animation: secondGlow 2s ease-in-out infinite alternate;\n}\n\n@keyframes secondGlow {\n  from {\n    box-shadow: \n      0 0 10px rgba(255, 51, 102, 0.5),\n      0 5px 15px rgba(0, 0, 0, 0.3),\n      0 0 20px rgba(255, 0, 51, 0.3) inset;\n  }\n  to {\n    box-shadow: \n      0 0 20px rgba(255, 51, 102, 0.8),\n      0 5px 20px rgba(0, 0, 0, 0.4),\n      0 0 40px rgba(255, 0, 51, 0.5) inset;\n  }\n}\n\n/* 毫秒卡片特殊样式 */\n.millisecond {\n  width: 100px;\n}\n\n.millisecond-front, .millisecond-back {\n  background: linear-gradient(135deg, #66ccff 0%, #3399ff 100%);\n  color: white;\n  text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3), 0 0 10px rgba(255, 255, 255, 0.5);\n  box-shadow: \n    0 0 15px rgba(102, 204, 255, 0.7),\n    0 5px 15px rgba(0, 0, 0, 0.3),\n    0 0 30px rgba(51, 153, 255, 0.5) inset;\n  animation: millisecondGlow 1s ease-in-out infinite alternate;\n}\n\n@keyframes millisecondGlow {\n  from {\n    box-shadow: \n      0 0 10px rgba(102, 204, 255, 0.7),\n      0 5px 15px rgba(0, 0, 0, 0.3),\n      0 0 20px rgba(51, 153, 255, 0.5) inset;\n  }\n  to {\n    box-shadow: \n      0 0 25px rgba(102, 204, 255, 1),\n      0 0 40px rgba(51, 153, 255, 0.8),\n      0 0 50px rgba(51, 153, 255, 0.7) inset;\n  }\n}\n\n/* 单位标签 */\n.flip-label {\n  @apply text-sm mt-3 opacity-80 tracking-wider uppercase;\n  font-weight: 700;\n  letter-spacing: 1.5px;\n  text-shadow: 0 1px 3px rgba(0, 0, 0, 0.5);\n  background: linear-gradient(90deg, #ff3366, #66ccff, #ffcc00);\n  background-size: 200% auto;\n  -webkit-background-clip: text;\n  background-clip: text;\n  color: transparent;\n  -webkit-text-fill-color: transparent;\n  animation: gradientFlow 5s ease infinite;\n}\n\n/* 分隔符 */\n.separator {\n  @apply text-4xl md:text-5xl font-bold;\n  color: var(--glow-accent);\n  text-shadow: 0 0 15px rgba(255, 204, 0, 0.8), 0 0 30px rgba(255, 153, 0, 0.6), 0 0 50px rgba(255, 153, 0, 0.4);\n  animation: blink 1s infinite alternate, bounce 2s ease-in-out infinite;\n  filter: blur(0.5px);\n}\n\n@keyframes blink {\n  0%, 50% {\n    opacity: 0.7;\n    transform: scale(0.95);\n  }\n  51%, 100% {\n    opacity: 1;\n    transform: scale(1.05);\n  }\n}\n\n@keyframes bounce {\n  0%, 100% {\n    transform: translateY(0);\n  }\n  50% {\n    transform: translateY(-5px);\n  }\n}\n\n/* 全局背景装饰 */\n.flip-box::after {\n  content: '';\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  width: 200%;\n  height: 200%;\n  background: radial-gradient(circle at center, rgba(255, 255, 255, 0.1) 0%, transparent 70%);\n  transform: translate(-50%, -50%);\n  pointer-events: none;\n  z-index: -1;\n}\n\n/* 响应式优化 */\n@media (max-width: 768px) {\n  .flip-card {\n    width: 60px;\n    height: 90px;\n  }\n  \n  .millisecond {\n    width: 80px;\n  }\n  \n  .flip-card-front, .flip-card-back {\n    font-size: 1.8rem;\n  }\n  \n  .separator {\n    font-size: 2.5rem;\n  }\n  \n  .flip-label {\n    font-size: 0.75rem;\n  }\n}\n</style>"],"mappings":";;EACOA,KAAK,EAAC;AAAiD;;EAEvDA,KAAK,EAAC;AAAoF;;EAKxFA,KAAK,EAAC;AAA0C;;EAE9CA,KAAK,EAAC;AAAU;;EACdA,KAAK,EAAC;AAAW;;EACfA,KAAK,EAAC;AAAiB;;EACrBA,KAAK,EAAC;AAA8B;;EACpCA,KAAK,EAAC;AAA4B;;EAGrCA,KAAK,EAAC;AAAY;;EAMrBA,KAAK,EAAC;AAAU;;EACdA,KAAK,EAAC;AAAW;;EACfA,KAAK,EAAC;AAAiB;;EACrBA,KAAK,EAAC;AAA8B;;EACpCA,KAAK,EAAC;AAA4B;;EAGrCA,KAAK,EAAC;AAAY;;EAMrBA,KAAK,EAAC;AAAU;;EACdA,KAAK,EAAC;AAAu